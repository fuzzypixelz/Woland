let isPrime: I64 -> Bool = do |n|
  @typeck_ignore
  var prime: Bool = true
  var i: I64 = 2
  loop
    if @cmp i n then 
      break
    end
    if @cmp (@mod n i) 0 then
      prime = false
    end
    i = @add i 1
  end
  prime
end

let main: String -> Void ~ do |argv|
  @typeck_ignore
  var i: I64 = 2
  loop
    if @cmp i 1000 then
      break
    end
    if isPrime i then
      @dump i
    end
    i = @add i 1
  end
end
