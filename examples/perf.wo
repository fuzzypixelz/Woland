let dump: I64 -> Void ~ do |a|
    @dump a
end

let add: I64 -> I64 -> I64 = do |x|
    let _: I64 -> I64 = do |y|
        @typeck_ignore
        @add x y
    end
end 

let cmp: I64 -> I64 -> Bool = do |x|
    let _: I64 -> Bool = do |y|
        @typeck_ignore
        @cmp x y
    end
end 

let main: String -> Void ~ do |args|
    var i: I64 = 0
    loop
        if cmp i 50000 then 
            break 
        else 
            ... 
        end
        i = add i 2
    end
end
